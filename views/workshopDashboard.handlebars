<div class="flex flex-col w-full md:flex-row grow">
  <aside id="default-sidebar" class="h-full left-0 text-brown z-40 md:w-64 transition-transform " aria-label="Sidebar">
    <div class="h-full px-3 py-4 overflow-y-auto bg-m-darkCream">
      <ul class="flex md:flex-col md:space-y-8 md:font-medium sm:pt-3 text-m-green">
        <li id="job-link"
          class="flex items-center p-2 rounded-lg cursor-pointer select-none hover:bg-m-cream text-m-green">
          <svg aria-hidden="true"
            class="w-6 h-6 transition duration-75 dark:text-gray-400 group-hover:dark:group-hover:text-white"
            fill="currentColor" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg">
            <path d="M2 10a8 8 0 018-8v8h8a8 8 0 11-16 0z"></path>
            <path d="M12 2.252A8.014 8.014 0 0117.748 8H12V2.252z"></path>
          </svg>
          <span class="ml-3 text-m-light-green">Job List</span>
        </li>
        <li id="customer-link"
          class="flex items-center p-2 rounded-lg cursor-pointer select-none hover:bg-m-cream text-m-green hover:text-black">
          <svg aria-hidden="true"
            class="flex-shrink-0 w-6 h-6 transition duration-75 dark:text-gray-400 group-hover:dark:group-hover:text-white"
            fill="currentColor" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg">
            <path
              d="M5 3a2 2 0 00-2 2v2a2 2 0 002 2h2a2 2 0 002-2V5a2 2 0 00-2-2H5zM5 11a2 2 0 00-2 2v2a2 2 0 002 2h2a2 2 0 002-2v-2a2 2 0 00-2-2H5zM11 5a2 2 0 012-2h2a2 2 0 012 2v2a2 2 0 01-2 2h-2a2 2 0 01-2-2V5zM11 13a2 2 0 012-2h2a2 2 0 012 2v2a2 2 0 01-2 2h-2a2 2 0 01-2-2v-2z">
            </path>
          </svg>
          <span class="flex-1 ml-3 whitespace-nowrap text-m-green">Customers</span>
          <span
            class="inline-flex items-center justify-center px-2 ml-3 text-sm font-medium text-gray-800 bg-gray-200 rounded-full dark:bg-gray-700 dark:text-gray-300"></span>
        </li>
        <li id="technician-link"
          class="flex items-center p-2 rounded-lg cursor-pointer select-none hover:bg-m-cream text-m-green">
          <svg aria-hidden="true"
            class="flex-shrink-0 w-6 h-6 transition duration-75 dark:text-gray-400 group-hover:dark:group-hover:text-white"
            fill="currentColor" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg">
            <path
              d="M8.707 7.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l2-2a1 1 0 00-1.414-1.414L11 7.586V3a1 1 0 10-2 0v4.586l-.293-.293z">
            </path>
            <path
              d="M3 5a2 2 0 012-2h1a1 1 0 010 2H5v7h2l1 2h4l1-2h2V5h-1a1 1 0 110-2h1a2 2 0 012 2v10a2 2 0 01-2 2H5a2 2 0 01-2-2V5z">
            </path>
          </svg>
          <span class="flex-1 ml-3 whitespace-nowrap text-m-green">Technicians</span>
        </li>
        <li id="add-technician-link"
          class="flex items-center p-2 rounded-lg cursor-pointer select-none hover:bg-m-cream text-m-green">
          <svg aria-hidden="true"
            class="flex-shrink-0 w-6 h-6 transition duration-75 dark:text-gray-400 group-hover:dark:group-hover:text-white"
            fill="currentColor" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg">
            <path fill-rule="evenodd" d="M10 9a3 3 0 100-6 3 3 0 000 6zm-7 9a7 7 0 1114 0H3z" clip-rule="evenodd">
            </path>
          </svg>
          <span class="flex-1 ml-3 whitespace-nowrap text-m-green">Add Technician</span>
        </li>

      </ul>
    </div>
  </aside>
  <section class="p-3 w-full bg-m-cream">
    <div id="jobs-section" class="block overflow-hidden overflow-y-auto h-full">
      {{> jobs }}
    </div>
    <div id="customer-section" class="hidden overflow-hidden overflow-y-auto h-full ">
      {{> customers }}
    </div>
    <div id="technician-section" class="hidden overflow-hidden overflow-y-auto h-full ">
      {{> technicians }}
    </div>
    <div id="add-technician-section" class="hidden overflow-hidden overflow-y-auto h-full ">
      {{> addTechnician }}
    </div>

  </section>
